<h1 class="content-title"><%= title %></h1>

<div class="list">
  <ul>
    <% gameinstance_list.sort(function(a, b) { %>
      <% let nameA = a.game.name.toUpperCase(); %>
      <% let nameB = b.game.name.toUpperCase(); %>
      <% return (nameA < nameB) ? -1 : (nameA > nameB) ? 1 : 0; %>
    <% }); %>
    <% gameinstance_list.forEach(gameinstance => { %>
      <a href="<%= gameinstance.url %>">
        <li class="list-item"><%= gameinstance.game.name %> ($<%= gameinstance.price %>)</li>
      </a>
    <% }) %>
  </ul>
</div>